
CREATE TABLE `addresses` (
	`address_id` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`addres_line1` VARCHAR(255) NULL DEFAULT NULL,
	`addres_line2` VARCHAR(255) NULL DEFAULT NULL,
	`addres_line3` VARCHAR(255) NULL DEFAULT NULL,
	`city` VARCHAR(255) NULL DEFAULT NULL,
	`country` VARCHAR(255) NULL DEFAULT NULL,
	`state` VARCHAR(255) NULL DEFAULT NULL,
	`zipcode` VARCHAR(255) NULL DEFAULT NULL,
	PRIMARY KEY (`address_id`)
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
AUTO_INCREMENT=15
;


CREATE TABLE `vendortypes` (
	`vendorType_id` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`vendorType_name` VARCHAR(255) NULL DEFAULT NULL,
	PRIMARY KEY (`vendorType_id`),
	UNIQUE INDEX `UK_9g65j9ooid5ji81uy7w7j61b8` (`vendorType_name`)
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
AUTO_INCREMENT=3
;



CREATE TABLE `vendors` (
	`vendor_id` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`bank_account_number` VARCHAR(255) NULL DEFAULT NULL,
	`bank_branch_ifsc` VARCHAR(255) NULL DEFAULT NULL,
	`bank_branch_name` VARCHAR(255) NULL DEFAULT NULL,
	`bank_name` VARCHAR(255) NULL DEFAULT NULL,
	`contact_person_alternate_phone_no` VARCHAR(255) NULL DEFAULT NULL,
	`contact_person_email` VARCHAR(255) NULL DEFAULT NULL,
	`contact_person_mobile` VARCHAR(255) NULL DEFAULT NULL,
	`contact_person_name` VARCHAR(255) NULL DEFAULT NULL,
	`serviceRendered_GoodsSupplied` VARCHAR(255) NULL DEFAULT NULL,
	`vendor_gst_number` VARCHAR(255) NULL DEFAULT NULL,
	`vendor_name` VARCHAR(255) NULL DEFAULT NULL,
	`vendor_nickName` VARCHAR(255) NULL DEFAULT NULL,
	`address_id` BIGINT(20) NULL DEFAULT NULL,
	`vendor_type_id` BIGINT(20) NULL DEFAULT NULL,
	PRIMARY KEY (`vendor_id`),
	INDEX `FK_vendors_vendortypes` (`vendor_type_id`),
	INDEX `FK_vendors_addresses` (`address_id`),
	CONSTRAINT `FK_vendors_addresses` FOREIGN KEY (`address_id`) REFERENCES `addresses` (`address_id`),
	CONSTRAINT `FK_vendors_vendortypes` FOREIGN KEY (`vendor_type_id`) REFERENCES `vendortypes` (`vendorType_id`)
)
COLLATE='latin1_swedish_ci'
ENGINE=InnoDB
AUTO_INCREMENT=5
;
